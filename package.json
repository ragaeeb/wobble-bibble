{
    "author": "Ragaeeb Haq",
    "bugs": {
        "url": "https://github.com/ragaeeb/wobble-bibble/issues"
    },
    "dependencies": {},
    "description": "TypeScript library for Islamic text translation prompts with LLM output validation and prompt stacking utilities.",
    "devDependencies": {
        "@biomejs/biome": "^2.3.14",
        "@types/bun": "^1.3.8",
        "@types/node": "^25.2.3",
        "semantic-release": "^25.0.3",
        "ts-to-zod": "^5.1.0",
        "tsdown": "^0.20.3",
        "typescript": "^5.9.3",
        "zod": "^4.3.6"
    },
    "engines": {
        "bun": ">=1.3.9",
        "node": ">=24.0.0"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**"
    ],
    "homepage": "https://github.com/ragaeeb/wobble-bibble",
    "keywords": [
        "islamic-translation",
        "llm-prompts",
        "arabic-english",
        "hadith",
        "fiqh",
        "tafsir",
        "prompt-engineering",
        "validation",
        "typescript"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "name": "wobble-bibble",
    "packageManager": "bun@1.3.9",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/wobble-bibble.git"
    },
    "scripts": {
        "build": "bun run generate && bunx tsc -p tsconfig.build.json --noEmit && tsdown",
        "generate": "bun run scripts/generate-prompts.ts",
        "generate:schemas": "ts-to-zod scripts/types/chatgpt.ts src/schemas/chatgpt.zod.ts && ts-to-zod src/types/gemini.ts src/schemas/gemini.zod.ts && ts-to-zod src/types/grok.ts src/schemas/grok.zod.ts",
        "lint": "biome check .",
        "test": "bun test src/ tests/",
        "test:dist": "bun run build && bun test tests/dist.test.ts"
    },
    "sideEffects": false,
    "source": "src/index.ts",
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "1.4.3"
}
