{
    "name": "wobble-bibble",
    "version": "1.0.1",
    "author": "Ragaeeb Haq",
    "description": "TypeScript library for Islamic text translation prompts with LLM output validation and prompt stacking utilities.",
    "license": "MIT",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**"
    ],
    "workspaces": [
        "agents/*",
        "scripts/*"
    ],
    "scripts": {
        "build": "bun run generate && tsdown",
        "generate": "bun run scripts/generate-prompts.ts",
        "lint": "biome check .",
        "test": "bun test src/ tests/",
        "test:dist": "bun run build && bun test tests/dist.test.ts"
    },
    "engines": {
        "bun": ">=1.3.6",
        "node": ">=24.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/wobble-bibble.git"
    },
    "bugs": {
        "url": "https://github.com/ragaeeb/wobble-bibble/issues"
    },
    "homepage": "https://github.com/ragaeeb/wobble-bibble",
    "keywords": [
        "islamic-translation",
        "llm-prompts",
        "arabic-english",
        "hadith",
        "fiqh",
        "tafsir",
        "prompt-engineering",
        "validation",
        "typescript"
    ],
    "packageManager": "bun@1.3.6",
    "dependencies": {
        "@langchain/openai": "^1.2.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.11",
        "@types/bun": "latest",
        "@types/node": "^25.0.9",
        "semantic-release": "^25.0.2",
        "tsdown": "^0.6.1",
        "typescript": "^5.9.3"
    }
}
