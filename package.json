{
    "author": "Ragaeeb Haq",
    "bugs": {
        "url": "https://github.com/ragaeeb/wobble-bibble/issues"
    },
    "dependencies": {
        "@langchain/openai": "^1.2.2"
    },
    "description": "TypeScript library for Islamic text translation prompts with LLM output validation and prompt stacking utilities.",
    "devDependencies": {
        "@biomejs/biome": "^2.3.11",
        "@types/bun": "^1.3.6",
        "@types/node": "^25.0.9",
        "semantic-release": "^25.0.2",
        "tsdown": "^0.20.0-beta.3",
        "typescript": "^5.9.3"
    },
    "engines": {
        "bun": ">=1.3.6",
        "node": ">=24.0.0"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**"
    ],
    "homepage": "https://github.com/ragaeeb/wobble-bibble",
    "keywords": [
        "islamic-translation",
        "llm-prompts",
        "arabic-english",
        "hadith",
        "fiqh",
        "tafsir",
        "prompt-engineering",
        "validation",
        "typescript"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "name": "wobble-bibble",
    "packageManager": "bun@1.3.6",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/wobble-bibble.git"
    },
    "scripts": {
        "build": "bun run generate && bunx tsc -p tsconfig.build.json --noEmit && tsdown",
        "generate": "bun run scripts/generate-prompts.ts",
        "lint": "biome check .",
        "test": "bun test src/ tests/",
        "test:dist": "bun run build && bun test tests/dist.test.ts"
    },
    "sideEffects": false,
    "source": "src/index.ts",
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "1.2.0",
    "workspaces": [
        "agents/*",
        "scripts/*"
    ]
}
